import java.util.HashSet;

public class PhoneNumber {
		private String areaCode;
		private String prefix;
		private String number;
		
		public PhoneNumber(String deadpool){
			String phoneNumber = deadpool;
			areaCode = deadpool.substring(0,2);
			prefix = deadpool.substring(3, 5);
			number = deadpool.substring(6, 9);
		}	
		public String toString(){
			return areaCode + "-" + prefix + "-" + number;
		}
		
		public addEntry(){
			
		}
		
		public String getAreaCode(String t){
			return areaCode;
		}
		
		public String getPrefix(){
			return prefix;
		}
		
		public String getNumber(){
			return number;
		}
}	




