
public class TicketMachine {
// fields/attributes
private int cost;
private int money_inserted ;	
//constructor
	public TicketMachine() {
	cost = 89 ;
	money_inserted = 0 ;
	
	}
//accessor methods
	public int getCost(){
	return cost;	
	}
	public int getCashInserted() {
		return money_inserted;
	}
//mutator methods	
	public void insertCash(int yen){
		if(yen >= 0);
			yen = money_inserted;
		}
	


}
